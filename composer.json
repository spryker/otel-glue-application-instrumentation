{
    "name": "spryker/otel-glue-application-instrumentation",
    "type": "library",
    "description": "OtelGlueApplicationInstrumentation module",
    "license": "proprietary",
    "require": {
        "open-telemetry/api": "*",
        "open-telemetry/context": "*",
        "open-telemetry/sem-conv": "*",
        "php": ">=8.1",
        "spryker/glue-application": "*",
        "spryker/kernel": "*",
        "spryker/opentelemetry": "*",
        "spryker/symfony": "*"
    },
    "require-dev": {
        "open-telemetry/sdk": "*"
    },
    "autoload": {
        "psr-4": {
            "Spryker\\": "src/Spryker/"
        },
        "files": [
            "_register.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "SprykerTest\\": "tests/SprykerTest/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {},
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
